{
    "name":"asconsulting/contao_states",
    "description":"A basic States extensions for Contao CMS 4.9+",
    "homepage": "https://andrewstevens.consulting",
    "keywords":["contao", "states", "provinces"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Andrew Stevens",
            "homepage":"https://andrewstevens.consulting"
        }
    ],
    "require":{
        "php": "^7.2",
        "contao/core-bundle": "^4.9"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
	"autoload": {
        "psr-4": {
            "Contao\\": [
                "system/modules/contao_states/library/Contao"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/contao_states": "system/modules/contao_states"
            }
        }
    },
    "minimum-stability": "dev"
}
